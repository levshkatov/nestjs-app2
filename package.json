{
  "name": "nestjs-app2",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production nest start",
    "start:lev": "NODE_ENV=lev nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "git:master": "git checkout master && git pull origin master && git push origin master",
    "git:deploy": "git checkout deploy && git pull origin deploy && git merge --no-edit master && git push origin deploy && git checkout master",
    "git:prod": "npm run git:master && git checkout prod && git pull origin prod && git merge --no-edit master && git push origin prod && git checkout master",
    "git:all": "npm run git:master && npm run git:deploy"
  },
  "dependencies": {
    "@nestjs/common": "7.5.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "7.5.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.5.5",
    "@nestjs/passport": "^7.1.4",
    "@nestjs/platform-express": "^7.5.5",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/sequelize": "0.1.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.5",
    "bcrypt": "^5.0.0",
    "blurhash": "^1.1.3",
    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "easy-yandex-s3": "^1.1.5",
    "easyvk": "^2.8.2",
    "firebase-admin": "^9.4.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nats": "^1.4.12",
    "nestjs-i18n": "^8.0.10",
    "node-geocoder": "^3.27.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "reflect-metadata": "0.1.13",
    "response-time": "^2.3.2",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "sendpulse-api": "^1.1.4",
    "sequelize": "5.22.3",
